<?xml version="1.0" encoding="UTF-8"?>
<?eclipse version="3.4"?>

<plugin>
   	<extension point="org.knime.workbench.repository.nodes">
   		<node category-path="/scripting/python" factory-class="org.knime.python.nodes.variables.PythonVariablesNodeFactory" />
   		<node category-path="/scripting/python" factory-class="org.knime.python.nodes.source.PythonSourceNodeFactory" after="org.knime.python.nodes.variables.PythonVariablesNodeFactory"/>
   		<node category-path="/scripting/python" factory-class="org.knime.python.nodes.script.PythonScriptNodeFactory" after="org.knime.python.nodes.source.PythonSourceNodeFactory"/>
        <node category-path="/scripting/python" factory-class="org.knime.python.nodes.script1in2out.PythonScript1In2OutNodeFactory" after="org.knime.python.nodes.script.PythonScriptNodeFactory"/>         
   		<node category-path="/scripting/python" factory-class="org.knime.python.nodes.script2in1out.PythonScript2In1OutNodeFactory" after="org.knime.python.nodes.script1in2out.PythonScript1In2OutNodeFactory"/>  		
        <node category-path="/scripting/python" factory-class="org.knime.python.nodes.script2in2out.PythonScript2In2OutNodeFactory" after="org.knime.python.nodes.script2in1out.PythonScript2In1OutNodeFactory"/>
   		<node category-path="/scripting/python" factory-class="org.knime.python.nodes.view.PythonViewNodeFactory" after="org.knime.python.nodes.script2in2out.PythonScript2In2OutNodeFactory"/>
   		<node category-path="/scripting/python" factory-class="org.knime.python.nodes.objectreader.PythonObjectReaderNodeFactory" after="org.knime.python.nodes.view.PythonViewNodeFactory"/>
   		<node category-path="/scripting/python" factory-class="org.knime.python.nodes.objectwriter.PythonObjectWriterNodeFactory" after="org.knime.python.nodes.objectreader.PythonObjectReaderNodeFactory"/>
   		<node category-path="/scripting/python" factory-class="org.knime.python.nodes.learner.PythonLearnerNodeFactory" after="org.knime.python.nodes.objectwriter.PythonObjectWriterNodeFactory"/>
   		<node category-path="/scripting/python" factory-class="org.knime.python.nodes.predictor.PythonPredictorNodeFactory" after="org.knime.python.nodes.learner.PythonLearnerNodeFactory"/>
        <node category-path="/scripting/python" factory-class="org.knime.python.nodes.db.PythonScriptDBNodeFactory" />
	</extension>
	<extension point="org.knime.workbench.repository.categories">
      	<category description="Python nodes" icon="icons/python.png" level-id="python" name="Python" path="/scripting" />
   	</extension>
     <extension
           point="org.knime.workbench.repository.registeredFileExtensions">
        <FileExtensionMapping
              NodeFactory="org.knime.python.nodes.objectreader.PythonObjectReaderNodeFactory">
           <FileExtension
                 extension=".pkl">
           </FileExtension>
        </FileExtensionMapping>
     </extension>
     <extension
           point="org.knime.python.sourcecodetemplates">
        <directory
              path="templates/">
        </directory>
     </extension>
</plugin>
